    "setup-python": "github:aminya/setup-python#9700887",
    "shx": "0.4.0",
    "simple-update-notifier": "^2.0.0",
    "terser": "^5.39.0",
    "terser-config-atomic": "^1.0.0",
    "time-delta": "github:aminya/time-delta#69d91a4",
    "timers-browserify": "^2.0.12",
    "ts-node": "^10.9.2",
