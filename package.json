    "safe-stable-stringify": "^2.5.0",
    "semver": "7.6.3",
    "setup-apt": "workspace:*",
    "setup-brew": "workspace:*",
    "setup-python": "github:aminya/setup-python#a783db655c6e40317e2c0c96f9d162d9c9f4a751",
    "shx": "0.3.4",
    "simple-update-notifier": "^2.0.0",
