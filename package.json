    "admina": "^1.0.1",
    "caxa": "^3.0.1",
    "ci-info": "^4.1.0",
    "ci-log": "workspace:*",
    "cross-env": "7.0.3",
    "cross-spawn": "^7.0.6",
    "cspell": "^8.17.5",
