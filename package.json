    "eslint": "^8.57.1",
    "eslint-config-atomic": "^1.22.1",
    "exec-powershell": "workspace:*",
    "execa": "^7.2.0",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "is-url-online": "^1.5.0",
    "jest": "^29.7.0",
    "lefthook": "^1.11.2",
    "macos-release": "^3.3.0",
    "memoizee": "^0.4.17",
    "mkdirp": "^3.0.1",
    "mri": "^1.2.0",
    "msvc-dev-cmd": "github:aminya/msvc-dev-cmd#c01f519bd995460228ed3dec4df51df92dc290fd",
    "node-downloader-helper": "2.1.9",
